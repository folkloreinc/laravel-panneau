{
    "name": "folklore/laravel-panneau",
    "description": "",
    "keywords": [
        "laravel"
    ],
    "homepage": "http://github.com/Folkloreatelier",
    "license": "MIT",
    "authors": [
        {
            "name": "Folklore",
            "email": "info@folklore.email",
            "homepage": "http://folkloreinc.ca"
        },
        {
            "name": "David Mongeau-Petitpas",
            "email": "dmp@folklore.email",
            "homepage": "http://mongo.ca",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/contracts": "^7.0|^8.0|^9.0",
        "illuminate/http": "^7.0|^8.0|^9.0",
        "illuminate/routing": "^7.0|^8.0|^9.0",
        "illuminate/support": "^7.0|^8.0|^9.0",
        "laravel/fortify": "^1.7"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "orchestra/database": "5.*|6.*|7.*",
        "orchestra/testbench": "5.*|6.*|7.*",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^8.5.8|^9.3.3",
        "php-coveralls/php-coveralls": "^2.4"
    },
    "suggest": {
        "illuminate/collections": "Required when using laravel 8.0"
    },
    "autoload": {
        "psr-0": {
            "Panneau": "src/"
        },
        "files": [
            "src/Panneau/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Panneau\\Tests\\": "tests/",
            "TestApp\\": "tests/fixture"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Panneau\\ServiceProvider"
            ],
            "aliases": {
                "Panneau": "Panneau\\Support\\Facade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
